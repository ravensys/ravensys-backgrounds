<background>
    <starttime>
        <year>@DATE_YEAR@</year>
        <month>@DATE_MONTH@</month>
        <day>@DATE_DAY@</day>
        <hour>00</hour>
        <minute>00</minute>
        <second>00</second>
    </starttime>
    <static>
        <duration>10000000000.0</duration>
        <file>
            <!-- Wide 16:9 -->
            <size width="1920" height="1080">@TVWIDE_IMAGE@</size>
            <!-- Wide 16:10 -->
            <size width="1920" height="1200">@WIDE_IMAGE@</size>
            <!-- Standard 4:3 -->
            <size width="2048" height="1536">@STANDARD_IMAGE@</size>
            <!-- Normalish 5:4 -->
            <size width="1280" height="1024">@NORMALISH_IMAGE@</size>
        </file>
    </static>
</background>

